pipeline{
    agent {
        label 'rhel'
    }
    tools{
    maven 'maven362'
    }
    
    stages{
        stage('checkout'){
         steps{
            checkout scm
     }
    }
    
    stage('Built'){
        steps{
            sh "mvn --version"
            sh "mvn clean install"
            }
            }
            stage('Test'){
            steps{
            sh "mvn test -DskipTests"
              
           }
           stage('post buils'){
            steps{

                sh "echo anil"

             

            }
           }
    }
}
}
