pipeline{
    agent {
        label 'rhel'
    }
    tools{
    maven 'maven362'
    }
    options {
       
          timeout(10)
  
  buildDiscarder logRotator(artifactDaysToKeepStr: '', artifactNumToKeepStr: '', daysToKeepStr: '5', numToKeepStr: '5')
}
    stages{
        stage('checkout'){
         steps{
            checkout scm
     }
    }
    
    stage('Built'){
        steps{
            sh "mvn --version"
            sh "mvn clean install-DskipTests "
            }
            }
            stage('Test'){
            steps{
            sh "mvn test"
             } 
           }
           stage('post buils'){
            steps{

                sh "echo EMAIl PRIvT"

             

            }
           }
    
}
}
